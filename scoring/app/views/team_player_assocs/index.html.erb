<h1>Listing team_player_assocs</h1>

<table>
  <tr>
    <th>Team</th>
    <th>Player</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @team_player_assocs.each do |team_player_assoc| %>
  <tr>
    <td><%= team_player_assoc.team_id %></td>
    <td><%= team_player_assoc.player_id %></td>
    <td><%= link_to 'Show', team_player_assoc %></td>
    <td><%= link_to 'Edit', edit_team_player_assoc_path(team_player_assoc) %></td>
    <td><%= link_to 'Destroy', team_player_assoc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

